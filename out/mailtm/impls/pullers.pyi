from ..abc.generic import Token
from ..abc.modals import Account, Domain, DomainPageView

__all__ = ['get', 'xget']

class get:
    def __init__(self) -> None: ...
    def create_account(self, address: str, password: str) -> Account | None: ...
    def get_account(self, account_id: str) -> Account | None: ...
    def delete_account(self, account_id: str) -> bool: ...
    def get_account_token(self, account_address: str, account_password: str) -> Token | None: ...
    def get_domain(self, domain_id: str) -> Domain | None: ...
    def get_domains(self) -> DomainPageView | None: ...

class xget:
    def __init__(self) -> None: ...
    async def create_account(self, address: str, password: str) -> Account | None: ...
    async def get_account(self, account_id: str) -> Account | None: ...
    async def delete_account(self, account_id: str) -> None: ...
    async def get_account_token(self, account_address: str, account_password: str) -> Token | None: ...
    async def get_domain(self, domain_id: str) -> Domain | None: ...
    async def get_domains(self) -> DomainPageView | None: ...
